{:
    [(&Int32, &Int32) -> Int32]
        ref_sum;
    
    [(Int32) -> (&Int32)]
        put_on_heap;
    
    [(Int32, Int32) -> Int32]
        sum;

    [(End) -> (IO)]
        main;
:}

std::debug;
std::putInt32;

sum := (a, b) -> a + b;
ref_sum := (a, b) -> sum(a, b);
put_on_heap := (a) -> a;

main := () -> putInt32(debug(), ref_sum(
    put_on_heap(2),
    put_on_heap(4)
));
